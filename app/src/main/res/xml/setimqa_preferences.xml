<?xml version="1.0" encoding="utf-8"?>
<androidx.preference.PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <EditTextPreference
        android:key="edit_server"
        android:title="Server URL"
        android:defaultValue="http://52.231.231.51:3080/"
        android:summary="Enter ServerURL"
        android:inputType="text" />

    <EditTextPreference
        android:key="edit_project"
        android:title="Project key"
        android:summary="Enter Project key"
        android:defaultValue="$2a$05$cTP2gOyauFVGhfwpB/hOGOeQbf0neBDdekRQuQ8l1sUkPPhojVeeu^#1U8389ATPE/szowZGlK27A=="
