    <EditTextPreference
        android:key="edit_customID"
        android:title="Custom ID"
        android:summary="Enter Custom ID (nullable)"
        android:inputType="text" />

    <EditTextPreference
        android:key="edit_customEmail"
        android:title="Custom E-mail"
        android:summary="Enter Custom E-mail (nullable)"
        android:inputType="text" />

    <EditTextPreference
        android:key="edit_customName"
        android:title="Custom name"
        android:summary="Enter Custom name (nullable)"
